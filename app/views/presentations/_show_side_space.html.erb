<h3>タイトル<h3>
<h3><%= @presentation.title %></h3>
<h3>目次</h3>
<div class="content-box-show">
  <% unless @contents == nil%>
    <% @contents.each do |content| %>
      <div>
        <%= link_to " #{content.page} :#{content.content }", presentation_path(@presentation.id, page: content.page) %>
      </div>
    <% end %>
  <% end %>
</div>
<h3>解答一覧</h3>
<div class="presentation-answer-box">
  <% @answers.each do |answer| %>
    <%= link_to answer.content, presentation_presentation_answer_path(presentation_id: answer.presentation_id, id: answer.id)%>
  <% end %>
</div>